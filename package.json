{
  "name": "memory-cache-ttl",
  "version": "1.0.10",
  "repository": {
    "type": "git",
    "url": "git@github.com:tiborv/memory-cache-ttl.git"
  },
  "description": "A simple in-memory cache with Time To Live functionality",
  "main": "index.js",
  "scripts": {
    "build": "node ./node_modules/babel-cli/bin/babel src --out-file dist/index.js",
    "ava": "./node_modules/ava/cli.js",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "test": "npm run lint && npm run ava"
  },
  "author": "Tibor Vukovic <m@tib.im>",
  "keywords": [
    "cache",
    "simple",
    "ttl"
  ],
  "license": "BSD-2-Clause",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^2.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
