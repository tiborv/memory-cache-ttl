{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "indent": [2, 2],
    "vars-on-top": 0,
    "consistent-return": 0,
    "no-var": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "object-shorthand": 0,
    "no-console": 0,
    "no-confusing-arrow": 0,
    "no-param-reassign": 0,
    "arrow-parens": 0,
    "no-plusplus": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "import/prefer-default-export": 0
  }
}
